{
    "version":2,
    "builds":[
        {
            "src":"./index.js",
            "use":"@now/node"
        }
    ],
    "router": [
        {
            "src": "/",
            "dest": "/public/index.html"
        },
        {
            "src": "/api/*",
            "dest": "/api/:value"
        }
    ]
}
